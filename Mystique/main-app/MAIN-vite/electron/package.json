{
  "name": "vite-project",
  "version": "0.0.1",
  "main": "main.js",
  "license": "gisor",
  "description": "Gisor's electron",
  "author": "Gisor Liu <gisor@9527.com>",
  "homepage": "https://github.com/owner/project#readme",
  "scripts": {
    "start": "chcp 65001 && electron .",
    "build-win": "electron-builder -w",
    "build-linux": "electron-builder -l"
  },
  "dependencies": {
    "better-sqlite3": "^8.3.0",
    "electron-is-dev": "^2.0.0",
    "mapserver-gisor": "^1.0.1"
  },
  "devDependencies": {
    "electron": "^23.2.0",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9"
  },
  "build": {
    "asar": true,
    "appId": "com.gisor.nb2023",
    "productName": "Uavdefense",
    "nsis": {
      "installerIcon": "assets/icon.ico",
      "installerLanguages": [
        "en_US",
        "zh_CN"
      ]
    },
    "win": {
      "icon": "assets/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "assets/icon.png",
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
